    "generate:mole": "plop --plopfile ./generators/plopfile.mjs molecules",
    "generate:orgs": "plop --plopfile ./generators/plopfile.mjs organisms",
    "postinstall": "husky",
    "lint": "eslint . --max-warnings=0 -f mo",
    "lint:fix": "eslint . --fix -f mo",
    "release": "semantic-release",
    "release:local": "semantic-release --no-ci",
    "sort:package": "sort-package-json",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "test:e2e:ci": "start-server-and-test start 3000 cy:run",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
